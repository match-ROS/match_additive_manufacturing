<launch>
    <arg name="world_T_base_topic" default="/mur620/UR10_l/global_tcp_pose"/>
    <arg name="base_T_ee_topic" default="nozzle_distance"/> <!-- TODO: could be done with static transform in future -->
    <arg name="world_T_ee_topic" default="/global_nozzle_pose"/>
    
    <node name="get_total_pose" pkg="ur_trajectory_follower" type="get_total_Pose.py" output="screen">
        <!-- Topic Remappings -->
        <remap from="world_T_base" to="$(arg world_T_base_topic)"/>
        <remap from="base_T_ee" to="$(arg base_T_ee_topic)"/>
        <remap from="world_T_ee" to="$(arg world_T_ee_topic)"/><!-- publisher -->

        <param name="world_frame" value="map"/> <!-- for publisher -->
    </node>
</launch>