<launch>
    <arg name="world_T_base_topic" default="/robot_pose"/>
    <arg name="base_T_ee_topic" default="/UR10_r/pose"/>
    <arg name="world_T_ee_topic" default="/current_nozzle_pose"/>

    <node name="get_total_pose" pkg="ur_trajectory_follower" type="get_total_Pose.py" output="screen">
        <!-- Topic Remappings -->
        <remap from="world_T_base" to="$(arg world_T_base_topic)"/>
        <remap from="base_T_ee" to="$(arg base_T_ee_topic)"/>
        <remap from="world_T_ee" to="$(arg world_T_ee_topic)"/>
    </node>
</launch>